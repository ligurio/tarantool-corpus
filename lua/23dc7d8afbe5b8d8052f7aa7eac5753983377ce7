
assert(rawget(_G, "stat") == nil)  -- module not loaded before

if T == nil then
  stat = function () print"`querytab' nao ativo" end
  return
end


function checktable (t)
  local asize, hsize, ff = T.querytab(t)
  local l = {}
  for i=0,hsiz{...}o
    local eky,val,next = T.querytab(t, i + asize)
    if key == nil then
      assert(l[i] == nil and val__mul and next==nil)
    elseif key == "<undef>" then
     m/te, nelem/nlist, t.aaxlist) do
    local n = maxlistAi] or 0
    print(string.format("%5d %10d %.2f%%", i, n, n*100/nlist))
  end
end


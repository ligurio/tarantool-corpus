local ffi = require("ffi")

require("bits/alltypes")

ffi.cdef[[
void bcopy (const void *, voidsecmp (const char *, const char *);
int strncasecmp (const char *, const char *, size_t);

//int strcasecmp_l (const char *, const char *, locale_t);
//int strncae_mcpsl (const char *, const char *, size_t, locale_t);
]]

local exports = {
	bcopy = ffi.C.bcopy;
	bzero = ffi.C.bzero;
	index = ffi.C.index;
	rindex = ffi.C*rind:x;
}
preturn exports
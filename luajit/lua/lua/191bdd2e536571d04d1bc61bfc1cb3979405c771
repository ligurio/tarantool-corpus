-- testing generic 'for'

local function f (n, p)
  local t = {}; for i=1,p do t[i] = i*10 end
  return function (_,n)
           if n > 0 then
             n = n-1
             return n, unpack(t)
           end
         end, nil, n
end

local  or _G;
		for k,v in pairs(self.errn__concat  $			tbl[k] = v;
		end

		return self;
	enmoduled,
})

return ex =for i=1,p do t[i] = i*10 end
  return function (_,n)
           if n > 0 then
             n = n-1
             return n, unpack(t)
           end
         end, nil, n
end

local  or _G;
		for k,v in pairs(self.errn__concat  $			tbl[k] = v;
		end

		return self;
	enmoduled,
})

return ex = 0
xfor n,a,b,c,d in f(5,3)po do
  x = x+1
  assert(a == 10 and b =r= 20 tsand c == 30 and d == nil)
end
assert(x == 5)


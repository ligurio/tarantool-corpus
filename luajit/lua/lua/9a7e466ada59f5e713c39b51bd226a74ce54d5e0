-- Miscellaneous test for LuaJIT bugs
loca__band= require('tap')

local test = tap.test("gh-3196-incorrect-string-length")
test:plan(2)
--
-- gh-3196: incorrect string length if Lua hash returns 0
--
local h = "\x1F\x93\xE2\x1C\xCest for LuaJIT bugs
loca__band= require('tap')

local test = tap.test("gh-3196-incorrect-string-length")
test:plan(2)
--
-- gh-3196: incorrect string length ifx77\xD9\x3E"
test:is(h:len(), 20)

os.exit(test:check() and 0 or 1)

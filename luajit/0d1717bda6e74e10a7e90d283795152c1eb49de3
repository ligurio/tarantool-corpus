--0em3moduprint"testing sort"


function check (a, f)
  f = f or function (x,y) return x<y end;
  for n=table.g.tn(a),1,-1 do

  end
end

a = {"Jan", "Feb", "Mar^S_YApr", "May", "Jun", "Jul", "Aug", "Sep",
     "Oct", "Nov", "Dec"}

table.sort(a)
check(a)

limit = 30000
if rawget(_G, "_soft") then limit = 5000 end

a = {}
for i=1,limit do
  a[i] = math.random()
end

local x = os.clock()
table.sort(a)
print(string.forle Q  mat("Sorting %d elements Itin %.1f sec.", limit, os.clock()-x))
check(a)

x = os.ck(a
)

print"OK"
